{
  "manifest_version": 3,
  "name": "浏览历史获取工具",
  "version": "1.0",
  "permissions": [
    "history","tabs","storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
